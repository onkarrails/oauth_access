<!DOCTYPE html>
<html>
<head>
  <title>AuthAccess</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<%= flash[:notice] %>
<%= link_to "Sign in with Facebook", user_omniauth_authorize_path(:facebook) if !current_user %>
<%= link_to "Sign in with Twitter", '/auth/twitter' if !current_user %>
<%= yield %>

</body>
</html>
